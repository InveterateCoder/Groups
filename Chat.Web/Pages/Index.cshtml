@page
@model Chat.Web.Pages.IndexModel
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <meta charset="utf-8" />
    <title>Chat</title>
    <link href="https://fonts.googleapis.com/css?family=Concert+One&display=swap" rel="stylesheet">
    <link asp-href-include="~/css/*" rel="stylesheet" />
    <script asp-src-include="~/js/*"></script>
</head>
<body>
    <div id="reg" class="disable-select">
        <div id="reg_panel">
            <div class="header">Groups</div>
            <div id="reg_panel_menu">
                <div>
                    <img src="~/images/register.svg" style="width:80%" draggable="false" />
                    <div>Register</div>
                </div>
                <div>
                    <img src="~/images/signin.svg" style="width:80%" draggable="false" />
                    <div>Sign In</div>
                </div>
            </div>
        </div>
    </div>
</body>
</html>